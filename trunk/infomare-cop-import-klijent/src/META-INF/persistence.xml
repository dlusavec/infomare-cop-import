<?xml version="1.0" encoding="windows-1250" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
  <persistence-unit name="infomare-cop-import-klijent">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>java:/app/jdbc/jdbc/COP_konekcijaDS</jta-data-source>
    <class>hr.infomare.cop.db.Doprinos</class>
    <class>hr.infomare.cop.db.Obracun</class>
    <class>hr.infomare.cop.db.Obustava</class>
    <class>hr.infomare.cop.db.Olaksica</class>
    <class>hr.infomare.cop.db.Param</class>
    <class>hr.infomare.cop.db.Poduzece</class>
    <class>hr.infomare.cop.db.Poripri</class>
    <class>hr.infomare.cop.db.Prihod</class>
    <class>hr.infomare.cop.db.Zaposl</class>
    <class>hr.infomare.cop.db.Zaprac</class>
    <properties>
      <property name="eclipselink.target-server" value="WebLogic_10"/>
      <property name="javax.persistence.jtaDataSource" value="java:/app/jdbc/jdbc/COP_konekcijaDS"/>
      <property name="javax.persistence.jdbc.driver" value="com.ibm.as400.access.AS400JDBCDriver"/>
    </properties>


  </persistence-unit>
</persistence>
